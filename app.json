{
  "expo": {
    "name": "mayani",
    "slug": "mayani",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/renner.jpeg",
    "scheme": "mayani",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.graysonwi.mayani"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/renner.jpeg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/renner.jpeg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "enableFullScreenImage_legacy": true
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
